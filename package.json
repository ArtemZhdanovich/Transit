{
  "name": "masstransit-docs",
  "version": "0.1.0",
  "description": "MassTransit Documentation",
  "main": "index.js",
  "devDependencies": {
    "@vuepress/plugin-active-header-links": "^1.0.0-rc.1",
    "vuepress": "^1.2.0",
    "@vuepress/plugin-blog": "^1.5.1"
  },
  "dependencies": {
    "remove-markdown": "^0.3.0"
  },
  "scripts": {
    "docs:build": "bash -c \"rm -rf docs/.vuepress/dist\" && vuepress build docs",
    "docs:publish": "npm run docs:build && cd docs/.vuepress/dist && git init && git commit --allow-empty -m \"Update docs\" && git checkout -b gh-pages && git add . && git commit -am \"Update docs\" && git push https://github.com/MassTransit/MassTransit.git gh-pages --force",
    "docs:dev": "vuepress dev docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MassTransit/MassTransit.git"
  },
