<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net452</TargetFrameworks>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\..\..\MassTransit.snk</AssemblyOriginatorKeyFile>
    <DebugType>portable</DebugType>
  </PropertyGroup>
  <PropertyGroup>
    <PackageId>MassTransit.Ninject</PackageId>
    <Description>MassTransit Ninject container support; $(Description)</Description>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\MassTransit.NinjectIntegration.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="GreenPipes" Version="1.0.10" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
    <PackageReference Include="Ninject" Version="3.2.2" />
    <PackageReference Include="Ninject.Extensions.NamedScope" Version="3.2.0" />
    <Reference Include="System" />
    <Reference Include="System.Core" />
	<ProjectReference Include="..\..\MassTransit\MassTransit.csproj" />
  </ItemGroup>
</Project>